.container-box{
    height:100vh;
}
.bg-img{
    width:100%;
    height:100%;
}
.service-qr-code{
    position: fixed;
    width:80%;
    left:0;
    right:0;
    margin:0 auto;
    top:27%;
    padding:100rpx 0 60rpx 0;
    border-radius:16rpx;
    background:#fff;
}
.qr-code{
    width:400rpx;
    height:400rpx;
}
.m-t-20{
    margin-top:20rpx;
}
.mobile-icon{
    width:48rpx;
    height:48rpx;
    margin-right:20rpx;
}
.mobile-num{
    margin-top:10rpx;
}
.service-mobile{
    padding:54rpx 42rpx 0 42rpx;
    border-top:1rpx solid rgba(228,228,228);
    
}
.service-mobile-right{
    width: 190rpx;
    height: 72rpx;
    line-height:72rpx;
    text-align:center;
    background: linear-gradient(rgba(252,86,57,255) 0%,rgba(252,132,57,255) 100%);
    border-radius: 70rpx;
    color:#fff;
}
.padding-b-60{
    padding-bottom:60rpx;
}