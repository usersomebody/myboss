.container{
    padding:0 30rpx;
    background:#fff;
}

.li{
    padding:28rpx 0 30rpx;
    border-bottom:2rpx solid #E2E3E8;
}

.border-none{
    border-bottom:none;
}

.li .item-value .value-placeholder{
    margin-right:16rpx;
}

.right-icon image{
    width:10rpx;
    height:18rpx;
}

.color-block{
    width:60rpx;
    height:30rpx;
}

.color-e83{
    color:#E83926!important;
}

.memo{
    width:516rpx;
    height:176rpx;
    padding:10rpx 20rpx;
    border:1rpx solid #C5C5C5;
    border-radius:4rpx;
}

.color-d2d{
    color:#D2D2D2;
}

.popup-box{
    position:fixed;
    top:0;
    left:0;
    width:100%;
    z-index:2001;
}

.popup-content{
    padding:24rpx 30rpx;
    border-radius:0 0 20rpx 20rpx;
}

.spceing{
    margin-top:16rpx;
    
}

.input-box{
    margin:20rpx 0 34rpx;
}

.input-box input{
    padding-left:13%;
    width: 70%;
    height:60rpx;
    line-height: 60rpx;
    text-indent: 10rpx;
    background:#F2F2F2;
    color:#333;
    border-radius:40rpx;
}

.searc-btn{
    margin-left:32rpx;
}

.list{
    flex-wrap: wrap;
}

.list .li-item{
    position: relative;
    width: 200rpx;
    height: 72rpx;
    line-height: 72rpx;
    border-radius:6rpx;
    background:#F2F2F2;
    margin-right:22rpx;
    margin-bottom:26rpx;
    box-sizing: border-box;
}

/* .list .li-item:nth-child(4n){
    margin-right:0;
} */

.list .li-item.active{
    background:#F2EEFF;
    border:2rpx solid #9169FB;
    color:#9169FB;
}

.saveBtn{
    position: relative;
    z-index:99;
    width: 550rpx;
    height: 82rpx;
    line-height: 82rpx;
    background: #7D49FB;
    text-align: center;
    border-radius: 10rpx;
    margin:18rpx auto 0;
}

.sign-symbol{
    position: absolute;
    right:0;
    top:0;
}

.sign-symbol image{
    width:20rpx;
    height:15rpx;
}

.search-place-style{
    color:#D2D2D2;
    padding-left:70rpx;
}

.add-btn{
    display:flex;
    align-items: center;
    justify-content: center;
    margin-top:60rpx;
}

.add-btn .btn{
    width: 238rpx;
    height: 66rpx;
    line-height:66rpx;
    text-align: center;
    border-radius: 10rpx;
    background: #7D49FB;
}
.add-btn .cancle{
    border:2rpx solid #7D49FB;
    background:#fff;
    color:#7D49FB;
    margin-right:56rpx;
}
.search-image-icon{
    position: absolute;
    left:40rpx;
    width:26rpx;
    height:26rpx;
    margin-right:20rpx;
}

.use-time-input{
    width:65%;
    padding-left:5%;
    height: 70rpx;
    line-height: 70rpx;
    border:1rpx solid #DCDFE6;
    border-radius:4rpx;
}
.use-time-place{
    color:#D2D2D2;
}
.disable{
    background:#999;
}
.color-504{
    color:#504766;
}

.date-cover .cover{
    width:38rpx;
    height:34rpx;
    margin-right:10rpx;
}
.limit-content{
    padding:30rpx 32rpx;
    background:#FCFBFF;
    border-radius:10rpx;
}
.limit-date-select{
    padding:24rpx 26rpx;
    border:1rpx solid #E6E6E6;
}
.color-504{
    color:#504766;
}
.limit-date-title{
    margin-bottom:24rpx;
}
.limit-date{
    margin-bottom:30rpx;
}