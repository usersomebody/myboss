.cancle-title{
    color:#E83926;
    text-align: center;
    padding-top:20rpx;
}
.cancle-list{
    padding:20rpx 30rpx;
}
.cancle-item{
    background:#fff;
    padding:30rpx;
    border-radius:10rpx;
    margin-bottom:20rpx;
}
.cancle-item .cancle-avatar{
    margin-right:62rpx;
}
.cancle-item .cancle-avatar image{
    width:112rpx;
    height:112rpx;
    border-radius:50%;
}
.booking-info{
    width:65%;
}
.booking-info .info-item{
    margin-bottom:16rpx;
}
.weChart{
    margin-right:38rpx;
}
.weChart image{
    width:34rpx;
    height:28rpx;
}
.phone image{
    width:30rpx;
    height:30rpx;
}
.color-e83{
    color:#E83926;
}

.action-sheet-box{
    position: fixed;
    top:0;
    width:100%;
    height:100%;
    z-index:2001;
    display:flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.box-content{
    width:70%;
    background:#fff;
    border-radius:16rpx;
}
.box-content .title{
    width:100%;
    padding-top:26rpx;
    text-align: center;
}
.content{
    width:450rpx;
    margin:28rpx auto 24rpx;
}
.content textarea{
    padding:16rpx;
    width:418rpx;
    height:172rpx;
    background:#F3F3F3;
    border-radius:10rpx;

}
.action-btn{
    margin-top:16rpx;
}
.next-msg{
    padding-left:34rpx;
}
.next-msg image{
    width:44rpx;
    height:44rpx;
    margin-right:20rpx;
}
.action-btn{
    width:100%;
    border-top:1rpx solid #E5E5E5;
}
.action-btn .btn{
    height:104rpx;
    line-height: 104rpx;
    text-align: center;
    flex:1;
}
.action-btn .btn:first-child{
    border-right:1rpx solid #E5E5E5;
}
.action-btn .cancle{
    width:49%;
}
.font-number{
    position: absolute;
    right:16rpx;
    bottom:16rpx;
}
.icon-style-active{
    transform:rotate(180deg);
}
.status{
    padding:0 30rpx;
}

.status-type{
    width: 130rpx;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    background: #9B77F4;
    border-radius: 30rpx;
    margin:20rpx 0;
}
.status-item{
    padding:30rpx 60rpx 30rpx 60rpx;
    border-bottom:1rpx solid #F5F5F5;
}
.status-item.active{
    color:#9B77F4!important;
}
.status-type-list{
    position: absolute;
    top:90rpx;
    right:30rpx;
    z-index:99;
    background:#fff;
    border-radius:4rpx;
    box-shadow: 0px 3px 7px 0px rgba(154, 154, 154, 0.35)
}
.selectIcon{
    margin-left:24rpx;
    width:14rpx;
    height:8rpx;
}
.no-list-data{
    padding-top:240rpx;
    margin:0 auto;
}
.no-list-data image{
    width:424rpx;
    height:360rpx;
    margin-bottom:20rpx;
}