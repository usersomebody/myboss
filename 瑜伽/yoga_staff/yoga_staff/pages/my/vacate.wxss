.container{
    /* padding:24rpx 30rpx; */
}

.booking-list{
    margin-top:20rpx;
}

.booking-item{
    background:#fff;
    margin-bottom:20rpx;
    border-radius:10rpx;
}

.booking-info{
    padding:30rpx 30rpx 42rpx 30rpx;
}

.booking-info .info .item{
    margin-bottom:10rpx;
}

.booking-info .info .item:last-child{
    margin-bottom:0;
}

.booking-info .booking-avatar image{
    width:112rpx;
    height: 112rpx;
    border-radius: 50%;
    margin-right: 62rpx;
}

.booking-btn{
    padding:22rpx 30rpx 22rpx 0;
    border-top:2rpx solid #F3F3F3;
}

.common-btn{
    width: 134rpx;
    height: 42rpx;
    line-height:42rpx;
    text-align: center;
    border-radius: 42rpx;
    background:#7D49FB;
    box-sizing: border-box;
}

.cancle{
    background: rgba(125, 73, 251, 0.1);
    border: 2rpx solid #7D49FB;
    color:#7D49FB;
    margin-right:34rpx;
}

.sign{
    color:#fff;
}
.switch-tab{
    position: fixed;
    top:0;
    width:100%;
    background:#fff;
}
.switch-list{
    width:50%;
    text-align: center;
}
.switch-item{
    display:inline-block;
    padding:30rpx 0;
    
}
.switch-item.active{
    color:#8F33FF;
    border-bottom:6rpx solid #8F33FF;
}

.action-sheet-box{
    position: fixed;
    top:0;
    width:100%;
    height:100%;
    z-index:2001;
    display:flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.box-content{
    width:70%;
    background:#fff;
    border-radius:16rpx;
}
.box-content .title{
    width:100%;
    padding-top:26rpx;
    text-align: center;
}
.content{
    width:450rpx;
    margin:28rpx auto 24rpx;
}
.content textarea{
    padding:16rpx;
    width:418rpx;
    height:172rpx;
    background:#F3F3F3;
    border-radius:10rpx;

}
.action-btn{
    margin-top:16rpx;
}
.next-msg{
    padding-left:34rpx;
}
.next-msg image{
    width:44rpx;
    height:44rpx;
    margin-right:20rpx;
}
.action-btn{
    width:100%;
    border-top:1rpx solid #E5E5E5;
}
.action-btn .btn{
    padding:32rpx 0;
    text-align: center;
    flex:1;
}
.action-btn .btn:first-child{
    border-right:1rpx solid #E5E5E5;
}
.action-btn .refuse-cancle{
    width:49%;
}
.font-number{
    position: absolute;
    right:16rpx;
    bottom:16rpx;
}
.audit-status-content{
    padding:16rpx 60rpx;
    border-top:2rpx solid #F3F3F3;
}
.audit-status image{
    width:34rpx;
    height:34rpx;
}
.audit-status:nth-child(1){
    margin-bottom:20rpx;
}
.audit-status-title{
    margin:0 16rpx;
}
.booking-list{
    padding-top:104rpx;
}
.list-no-data{
    padding-top:104rpx;
}
.list-no-data image{
    width: 290rpx;
    height: 200rpx;
    margin: 110rpx auto 20rpx;
}
.no-list-data{
    padding-top:240rpx;
    margin:0 auto;
}
.no-list-data image{
    width:424rpx;
    height:360rpx;
    margin-bottom:20rpx;
}   