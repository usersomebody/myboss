.wrap{
    position: absolute;
    top:170rpx;
    left:44rpx;
    width: 100rpx;
    height: 100rpx;
    overflow: hidden;
    transform: skewY(0) rotateZ(88deg) rotateX(8deg) rotateY(-79deg);
}
.circle{
    height: 100%;
    box-sizing: border-box;
    border: 8rpx solid #7D49FB;
    border-radius: 50%;
}
.circle:before{
    content: '';
    position: absolute;
    display: block;
    width: 100%;
    height: 50%;
    box-sizing: border-box;
    top: 50%;
    left: 0;
    border: 8rpx solid #7D49FB;
    border-top: transparent;
    border-radius:  0 0 50% 50%/ 0 0 100% 100%;
    z-index: 2;
}
.top-circle, .bottom-circle{
    position: absolute;
    left: 0px;
    width: 100%;
    height: 50%;
    box-sizing: border-box;
    background:rgba(255,255,255,.2);
}
.top-circle{
    top: 0;
    z-index: 1;
    transform-origin: center bottom;
    animation: 1s back-half linear 1s;
    animation-fill-mode: forwards;
}
.bottom-circle{
    z-index: 3;
    top: 50%;
    transform-origin: center top;
    animation: 1s front-half linear;
    animation-fill-mode: forwards;
}
@keyframes front-half{
    0%{
        transform: rotate( 0 );
    }
    99%{
        transform: rotate( 180deg );
        opacity: 1;
    }
    100%{
        transform: rotate( 180deg );
        opacity: 0;
    }
}
@keyframes back-half{
    0%{
        transform: rotate( 0 );
    }
    99%{
        transform: rotate( 180deg );
        opacity: 1;
    }
    100%{
        transform: rotate( 180deg );
        opacity: 0;
    }
}

