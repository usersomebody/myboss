@keyframes fade-in {  
    0% {
        left: 0;
        opacity:0;
    }/*初始状态 透明度为0*/  
    40% {
        left: 125rpx;
        opacity:0;
    }/*过渡状态 透明度为0*/  
    100% {
        left: 250rpx;
        opacity:1;
    }/*结束状态 透明度为1*/  
}  

.show-data-box {    
    animation: fade-in;/*动画名称*/  
    animation-duration: 1.5s;/*动画持续时间*/  
    -webkit-animation:fade-in 1.5s;/*针对webkit内核*/  
}  