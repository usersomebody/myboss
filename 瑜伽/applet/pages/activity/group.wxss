.container{
    background:#fff;
}

.scroll-height{
    height:93vh;
}

.scroll-height.iPhoneX{
    height:calc(93vh - 68rpx);
}

swiper{
    height:588rpx;
}

.swiper-content{
    position: relative;
}

.power-swiper image{
    display:block;
    width:100%;
}

/* 轮播控件 */
.play-control {
    position: absolute;
    right:38rpx;
    bottom:34rpx;
    text-align: center;
    font-size:12px;
    color:#fff;
    background:#3F3F3F;
}

.play-muted {
    /* bottom: 9.5%; */
    padding: 0 26rpx;
    height: 44rpx;
    z-index: 10;
    line-height: 44rpx;
    border-radius: 22rpx;
}

.back{
    width:16rpx;
    height:28rpx;
    left:20rpx;
    z-index:99;
    padding:20rpx;
}

.back image{
    width:16rpx;
    height:28rpx;
    transform: rotate(180deg);
}

/* 产品信息 */
.activity-info .product-info{
    overflow-y: hidden;
    padding-right:40rpx;
    background:#E83926;
    padding:20rpx 30rpx;
    
}

.activity-info .trapezoid-box{
    position: relative;
    margin-top:-38rpx;
}
.activity-info .trapezoid-sign{
    position: relative;
    width:100rpx;
    height:38rpx;
    background:#FB5E69;
    line-height: 38rpx;
    text-align: center;
}

.trapezoid{
    width: 30rpx;
    height: 0;
    border-bottom: 38rpx solid #FB5E69;
    border-left: 15rpx solid transparent;
    border-right: 15rpx solid transparent;
    margin-left:-38rpx;
}

.activity-info .product-info .info-left{
    padding:12rpx 0 20rpx;
}

.product-info .info-left .price{
    margin-right:26rpx;
    font-size:48rpx;
    margin-bottom:20rpx;
}

.product-info .info-left .sale .num{
    white-space: nowrap;
}

.triangle-right{
    width: 0;
    height: 0;
    border-top: 54rpx solid transparent;
    border-bottom: 54rpx solid transparent;
    border-left: 32rpx solid #FB5E69;
    margin-left:-2rpx;
}

.info-price-left{
    height:96rpx;
}

.card-info{
    padding:12rpx 30rpx 22rpx;
    border-radius:10rpx;
    margin:30rpx 0;
}

.card-info .title{
    line-height:48rpx;
    margin-bottom:16rpx;
}

.card-info .card-intro{
    padding:14rpx 22rpx;
    background:#FCFCFC;
    border-radius:6rpx;
}

.card-intro .valid{
    margin:30rpx 0;
}

.width-20{
    width:30%;
}

/* 课程 */
.card-detail{
    margin-bottom:28rpx;
    padding:24rpx 30rpx;
}

.card-detail-title{
    margin-bottom:24rpx;
}

.card-detail-create{
    margin:20rpx 0;
}

.course-support{
    padding:24rpx 30rpx;
    margin-bottom:28rpx;
    border-radius:10rpx;
}

.course-list-box{
    overflow-y:hidden;
    height:184rpx;
    padding:12rpx 4rpx;
}

.course-x-list{
    overflow-x: auto;
    white-space: nowrap;
}

.course-x-list image{
    display:block;
    width:220rpx;
    height:130rpx;
    margin-right:16rpx;
    border-radius:10rpx;
}


.list-item{
    display:inline-flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.course-nama{
    margin-top:10rpx;
    width:220rpx;
}

.course-list-box .li{
    margin-bottom:10rpx;
}
.course-list-box .li-item{
    width:100%;
}
.li-item image{
    width:100%;
}
.course-list-box .li:last-child{
    margin-bottom:0;
}

::-webkt-scrollbar{
    display: none;
}


::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}


.footer{
    height:7vh;
    align-items: center;
    align-content:center;
    background:#fff;
    overflow: hidden;
    white-space: nowrap;
    box-shadow: 0px 8px 60px 0px rgba(0, 0, 0, 0.04);
    border-top:1px solid #f7f7f7;
    padding:0 0 0 50rpx;
}

.footer .left{
    height:100%;
    text-align: center;
    margin-right:36rpx;
}

.footer .left image{
    width:36rpx;
    height:40rpx;
}

.footer .right{
    flex:1;
    height:100%;
    background:#E83926;
    text-align: center;
    margin-left:30rpx;
}

.footer.iPhoneX:after {
    /* height: calc(68rpx + 8vh); */
    position: absolute;
    bottom: 0;
    content: '';
    height: 68rpx;
}

/* 弹窗 */
.pay-fixed{
    position: fixed;
    left:0;
    top:0;
    right:0;
    bottom:0;
    margin:auto;
    z-index:2001;
    display:flex;
    flex-direction:column;
    justify-content: center;
    align-items: center;
}

.pay-box{
    position: relative;
    width:80%;
    background:#ffffff;
    padding:50rpx 30rpx;
    border-radius:30rpx;
}

.pay-box .title{
    margin-bottom:58rpx;
}

.pay-box .input-box{
    padding:26rpx 0;
    border-bottom:2rpx solid #E9E9E9;
    margin-bottom:20rpx;
}
.pay-box .code{
    position: relative;
    flex:1;
}

.pay-box .phone .label{
    display:block;
    width:20rpx;
    height:26rpx;
    margin-right:16rpx;
}

.pay-box .code .label{
    display:block;
    width:22rpx;
    margin-right:16rpx;
    height:28rpx;
}

.pay-box .nick-name .label{
    margin-right:16rpx;
    display:block;
    width:22rpx;
    height:26rpx;
}

.pay-box .sure-btn{
    padding:18rpx 0;
    border-radius:10rpx;
    margin-top:76rpx;
    width:58%;
    background:rgba(145,105,251,.5);
    border-radius:100rpx;
    margin:76rpx auto 0;
}

.authCode{
    position: absolute;
    right:0;
    top:0;
    bottom:0;
    margin:auto;
    width: 130rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    background:rgba(145,105,251,.5);
    font-size:24rpx;
    color:#fff;
    border-radius:8rpx;
}

.placeStyle{
    color:#666666;
    font-size:24rpx;
}

.activity-over .over-box{
    position: relative;
    width:80%;
    padding:16rpx 0 50rpx;
    border-radius:20rpx;
    background:#fff;
    flex-direction: column;
}

.closebtn{
